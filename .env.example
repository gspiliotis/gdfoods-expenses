# myDATA API Credentials
# Copy this file to .env and fill in your actual credentials

# User ID for myDATA API
MYDATA_USER_ID=your_user_id_here

# API Key for myDATA API
MYDATA_API_KEY=your_api_key_here

# Google Sheets Configuration
# The ID of your Google Spreadsheet (found in the URL)
# Example: https://docs.google.com/spreadsheets/d/SPREADSHEET_ID/edit
GOOGLE_SPREADSHEET_ID=your_spreadsheet_id_here

# Path to your Google Service Account credentials JSON file
# You can create this in Google Cloud Console > IAM & Admin > Service Accounts
GOOGLE_CREDENTIALS_FILE=google-credentials.json
